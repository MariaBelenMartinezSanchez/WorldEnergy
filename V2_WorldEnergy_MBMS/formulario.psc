Algoritmo formulario
	Definir nombre,correo, comentario Como Caracter;
	Definir telefono Como Entero;
	
	Escribir "Nombre completo";
	Leer nombre;
	
	Escribir "Teléfono";
	Leer telefono;
	
	Escribir "Correoelectrónico";
	Leer correo;
	
	Escribir "Ingrese su comentario o solicitud";
	Leer comentario;
	
	Escribir "Gracias por completar el formulario"
	Escribir "", nombre
	
FinAlgoritmo
